package ub1.core;

using ub1.util.PropertyTool;
using ub1.web.DomTools;

class Body extends Element {

	override function init() {
		var e = page.doc.domGetBody();
		e != null ? props = props.set(Element.ELEMENT_PROP, e) : null;
		super.init();
	}

}
